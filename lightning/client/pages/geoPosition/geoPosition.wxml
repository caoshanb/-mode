<map id="map" longitude="{{lnt}}" latitude="{{lat}}" scale="18" controls="{{controls}}" bindcontroltap="controltap" markers="{{markers}}" bindmarkertap="markertap" polyline="{{polyline}}" bindregionchange="regionchange" show-location style="width: 100%; height: 300px;"></map>
<view class="button">
    <button  type='primary' bindtap='positions'>获取目标所在地理位置</button>
</view>
