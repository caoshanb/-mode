.yi{
  color:yellow;
  background: orchid;
}
.images{width: 100%;}
.order {  
   height: 100rpx;  
   line-height: 120rpx;  
   text-align: center;  
}  
 .order .line {  
   display: inline-block;  
   width: 150rpx;  
   border-top: 1px solid #faac34 ; 
    vertical-align: middle;  
    margin: 0 15rpx;
}  
.order .txt {  
   color: #db810d;  
   vertical-align: middle;  
   font-size: 30rpx;
   font-weight: bold;
}  
.flex-wrp{
   text-align: center;
    height: 450rpx;
    background-color: #FFFFFF;
    margin-bottom: 60rpx;
     float: left;
}
.flex-item{
   text-align: center;
    width: 280rpx;
    height: 410rpx;
    display: inline-block;
    margin: 0 42rpx;
}
.flex-wrp  image{
  width: 100%;
  height: 95%;
}
.flex-wrp  text,.money{
  font-size: 26rpx;
}

.flex-wrp:after,.pinpai::after{display: block;content: "";clear:both}
.money{color: #469a59;font-size: 28rpx;}
.pinpai {width: 100%;height:400rpx;}
.pinpai .tuan{width: 100%;height: 100%;}
.dianhua{width: 45rpx;height: 45rpx;float: right;}
.dianhuas{width:20rpx;height: 45rpx;float: right;border-left: 1rpx solid #ccc;}
.titk{margin-top: 40rpx;}
.tuanInfo{border-bottom: 1rpx #ccc  solid; height: 70rpx;line-height: 70rpx;}
.icon{margin: 0 25rpx;}
.dizhi{font-size: 24rpx;}
.hout{float: right;font-size: 24rpx;color: firebrick;padding-top:6rpx; }
.qrcode{text-align: center;font-size: 26rpx;background: #f2f2f2;margin-top:30rpx;padding: 20rpx 0; color: #666}